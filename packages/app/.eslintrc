{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "example"],
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
    "example/i18n-literal": "error",
    "example/ban-import": "error",
    "example/no-undef-css-module-class": "error",
    "example/no-unused-css-module-class": "error"
  }
}
